
(DEFPACKAGE "case"
  (:USE "CL" "SB-EXT"))
(IN-PACKAGE "case")
(REQUIRE 'ASDF)
(IF (NOT (EQUAL (PACKAGE-NAME *PACKAGE*) "builtins"))
    (ASDF/OPERATE:LOAD-SYSTEM :MULE))
(DEFPARAMETER |a| 1)
(DEFPARAMETER A 2)
(ASSERT (EQUALP (+ |a| 1) A))
(LOOP FOR S BEING EACH PRESENT-SYMBOL IN *PACKAGE*
      WHEN (OR (FBOUNDP S) (BOUNDP S) (FIND-CLASS S NIL))
      DO (EXPORT S))