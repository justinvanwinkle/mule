
(EVAL-WHEN (:COMPILE-TOPLEVEL :LOAD-TOPLEVEL :EXECUTE)
  (UNLESS (FIND-PACKAGE "arithmetic_operators.py")
    (MAKE-PACKAGE "arithmetic_operators.py" :USE '("COMMON-LISP"))))
(IN-PACKAGE "arithmetic_operators.py")
(IF (NOT (EQUAL (PACKAGE-NAME *PACKAGE*) "builtins"))
    (LOAD "/home/jvanwink/repos/mule/build/lib/builtins.fasl"))
(USE-PACKAGE "builtins")
(ASSERT (EQUALP (+ (+ 1 1) 1) 3))
(ASSERT (EQUALP (* 2 2) 4))
(ASSERT (EQUALP (- 3 3) 0))
(ASSERT (< (- (/ 5 2) 2.5) 0.001))
(DEFPARAMETER |x| 3)
(DEFPARAMETER |y| 2)
(ASSERT (EQUALP (- (* |x| |y|) 7) (- 1)))
(ASSERT (EQUALP (- 7 (* |x| |y|)) 1))
(ASSERT (EQUALP (* (+ |x| |y|) 3) 15))
(ASSERT (EQUALP (* 3 (+ 1 2)) 9))
(ASSERT (EQUALP (+ (* 3 1) 2) 5))
(LOOP FOR S BEING EACH PRESENT-SYMBOL IN *PACKAGE*
      WHEN (OR (FBOUNDP S) (BOUNDP S) (FIND-CLASS S NIL))
      DO (EXPORT S))