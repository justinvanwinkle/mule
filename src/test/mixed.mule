

class CounterX:
    __slots__ = ('count', )

    def __init__(self):
        self.count = 0

    def increment(self):
        self.count = self.count + 1


def foo():
    c = CounterX()
    c.increment()
    while c.count < 1000:
        c.increment()
    return c.count

x = foo()

ASSERT(x == 1000)
