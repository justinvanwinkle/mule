(CL:EVAL-WHEN (:compile-toplevel :load-toplevel :execute)(CL:UNLESS (CL:FIND-PACKAGE "global_scope")(make-package "global_scope")(CL:USE-PACKAGE "builtins")))
;(proclaim '(optimize (speed 3)))
(CL:DEFPARAMETER |x| 1)
(CL:DEFUN |x_plus| (|delta|  ) 
(CL:SETF |x| (+ |x| |delta|)))
(|muleassert| (|__eq__| |x| 1) )
(|x_plus| 5 )
(|muleassert| (|__eq__| |x| 6) )
(|x_plus| (- 6 ) )
(|muleassert| (|__eq__| |x| 0) )
(|x_plus| (- 5 (- 3 )) )
(|muleassert| (|__eq__| |x| 8) )
(CL:LOOP FOR S BEING EACH PRESENT-SYMBOL IN CL:*PACKAGE*
   WHEN (OR (CL:FBOUNDP S)
            (CL:BOUNDP S)
            (CL:FIND-CLASS S NIL))
   DO (CL:EXPORT S))

